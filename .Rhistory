# data exploration and visualization
## data frame to create histogram of responses
df <- data.frame(THC$Stone)
## generates histogram of 'stoned' responses
ggplot(data = df, aes(x = THC.Stone)) +
geom_histogram(bins = 10) +
theme_classic() +
scale_x_continuous(breaks = 1:10) +
ylab('Count') +
xlab('Stoned Questionnaire Responses') +
theme(text = element_text(size = 8))
## summary statistics for 'stoned' response
summary(THC$Stone)
sd(THC$Stone)
## table displaying the count of each of the responses
table(THC$Stone)
## generates pirate plot of 'stoned' response by THC dose and drive order
pirateplot(Stone ~ Dose*Drive.Order, data = THC, cex.lab = 0.6,
cex.axis = 0.6, cex.names = 0.6)
## summary statistics of 'stoned' response for each dose level
tapply(THC$Stone, THC$Dose, summary)
tapply(THC$Stone, THC$Dose, sd)
## summary statistics of 'stoned' response for each drive order
tapply(THC$Stone, THC$Drive.Order, summary)
tapply(THC$Stone, THC$Drive.Order, sd)
## summary statistics of 'stoned' response for each combination of
## dose and drive order
tapply(THC$Stone, interaction(THC$Dose, THC$Drive.Order), summary)
## table displaying count of observations for each drive order
summary(interaction(THC$Dose, THC$Drive.Order))
## generates pirate plot of 'stoned' response by THC dose and day order
pirateplot(Stone ~ Dose*Day.Order, data = THC, cex.lab = 0.6,
cex.axis = 0.6, cex.names = 0.6)
## table displaying count of observations for each day order
summary(THC$Day.Order)
## summary statistics of 'stoned' response for each day order
tapply(THC$Stone, THC$Day.Order, summary)
tapply(THC$Stone, THC$Day.Order, sd)
## summary statistics of 'stoned' response for each combination of
## dose and day order
tapply(THC$Stone, interaction(THC$Dose, THC$Day.Order), summary)
## generates pirate plot of 'stoned' response by simulator
pirateplot(Stone ~ Sim, data = THC, cex.lab = 0.6,
cex.axis = 0.6, cex.names = 0.6)
## table displaying number of observations for each driving simulator
summary(THC$Sim)
## summary statistics of 'stoned' response for each driving simulator
tapply(THC$Stone, THC$Sim, summary)
tapply(THC$Stone, THC$Sim, sd)
