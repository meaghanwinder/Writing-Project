variable.name = "Question", value.name = "Response")
levels(dat$Question)
dat$Question <- factor(dat$Question)
# group the questions that were together in the questionnaire
dat$Question <- relevel(dat$Question, "Stressful")
dat$Question <- relevel(dat$Question, "Enjoyable")
dat$Question <- relevel(dat$Question, "Willingb")
dat$Question <- relevel(dat$Question, "Willing")
dat$Question <- relevel(dat$Question, "Risky")
